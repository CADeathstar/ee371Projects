    %vpi_call 2 37 "$monitor", "\011\011 %b\011 %b \011 %b \011 %b", v008b27b0_0, v008b2910_0, v012fd9d0_0, v012fa060_0, $time {0 0 0};
    %end;
    .thread T_5;
    .scope S_008b3ae8;
T_6 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v008b27b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v008b2910_0, 0, 1;
