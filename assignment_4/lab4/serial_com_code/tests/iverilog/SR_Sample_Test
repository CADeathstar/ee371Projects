L_008b4f40 .functor BUFZ 8, v008b0858_0, C4<00000000>, C4<00000000>, C4<00000000>;
v008b0b18_0 .net "SRclock", 0 0, v008b0908_0;  alias, 1 drivers
v008b07a8_0 .net "dataBus", 7 0, L_008b4f40;  alias, 1 drivers
