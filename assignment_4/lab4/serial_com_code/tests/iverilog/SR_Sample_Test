    .scope S_012f5460;
T_3 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v008b0858_0, 0, 8;
    %end;
    .thread T_3;
    .scope S_012f5460;
T_4 ;
    %wait E_008a4350;
    %load/vec4 v008b0858_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v008b0858_0, 0, 8;
    %load/vec4 v008b06f8_0;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v008b0858_0, 4, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_008b3ae8;
T_5 ;
