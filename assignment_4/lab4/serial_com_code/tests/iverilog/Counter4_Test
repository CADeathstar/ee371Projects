#! c:/iverilog-x86/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_009091d0 .scope module, "DFlipFlop" "DFlipFlop" 2 1;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "q"
    .port_info 1 /OUTPUT 1 "qBar"
    .port_info 2 /INPUT 1 "D"
    .port_info 3 /INPUT 1 "clk"
    .port_info 4 /INPUT 1 "rst"
L_0090cea8 .functor NOT 1, v00909038_0, C4<0>, C4<0>, C4<0>;
o00823fd4 .functor BUFZ 1, C4<z>; HiZ drive
v009092a0_0 .net "D", 0 0, o00823fd4;  0 drivers
o00823fec .functor BUFZ 1, C4<z>; HiZ drive
v0090a020_0 .net "clk", 0 0, o00823fec;  0 drivers
v00909038_0 .var "q", 0 0;
v00907ed8_0 .net "qBar", 0 0, L_0090cea8;  1 drivers
o00824034 .functor BUFZ 1, C4<z>; HiZ drive
v0090a978_0 .net "rst", 0 0, o00824034;  0 drivers
E_0090be00/0 .event negedge, v0090a978_0;
E_0090be00/1 .event posedge, v0090a020_0;
E_0090be00 .event/or E_0090be00/0, E_0090be00/1;
S_0090a8a8 .scope module, "testBench" "testBench" 3 5;
 .timescale 0 0;
v00821270_0 .net "clk", 0 0, v0090a9d0_0;  1 drivers
v008213d0_0 .net "q", 3 0, v008232b8_0;  1 drivers
v00821638_0 .net "rst", 0 0, v00822610_0;  1 drivers
S_00822490 .scope module, "aTester" "Tester" 3 10, 3 20 0, S_0090a8a8;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "q"
    .port_info 1 /OUTPUT 1 "clk"
    .port_info 2 /OUTPUT 1 "rst"
P_0090bf40 .param/l "stimDelay" 0 3 24, +C4<00000000000000000000000000010100>;
v0090a9d0_0 .var "clk", 0 0;
v00822560_0 .var/i "i", 31 0;
v008225b8_0 .net "q", 3 0, v008232b8_0;  alias, 1 drivers
v00822610_0 .var "rst", 0 0;
S_00823190 .scope module, "counter" "Counter4" 3 9, 4 1 0, S_0090a8a8;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "q"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "rst"
v00823260_0 .net "clk", 0 0, v0090a9d0_0;  alias, 1 drivers
v008232b8_0 .var "q", 3 0;
v00821218_0 .net "rst", 0 0, v00822610_0;  alias, 1 drivers
E_0090bf68 .event posedge, v00822610_0, v0090a9d0_0;
    .scope S_009091d0;
T_0 ;
    %wait E_0090be00;
    %load/vec4 v0090a978_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00909038_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v009092a0_0;
    %store/vec4 v00909038_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00823190;
T_1 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v008232b8_0, 0, 4;
    %end;
    .thread T_1;
    .scope S_00823190;
T_2 ;
    %wait E_0090bf68;
    %load/vec4 v00821218_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v008232b8_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v008232b8_0;
    %cmpi/ne 15, 0, 4;
    %jmp/0xz  T_2.2, 4;
    %load/vec4 v008232b8_0;
    %addi 1, 0, 4;
    %assign/vec4 v008232b8_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v008232b8_0, 0;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_00822490;
T_3 ;
    %vpi_call 3 29 "$display", "\011\011 clk rst \011 q \011 Time " {0 0 0};
    %vpi_call 3 30 "$monitor", "\011\011 %b\011 %b \011 %b", v0090a9d0_0, v00822610_0, v008225b8_0, $time {0 0 0};
    %end;
    .thread T_3;
    .scope S_00822490;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0090a9d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00822610_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0090a9d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00822610_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0090a9d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00822610_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v00822560_0, 0, 32;
T_4.0 ;
    %load/vec4 v00822560_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.1, 5;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0090a9d0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0090a9d0_0, 0, 1;
    %load/vec4 v00822560_0;
    %addi 1, 0, 32;
    %store/vec4 v00822560_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %delay 40, 0;
    %vpi_call 3 44 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0090a8a8;
T_5 ;
    %vpi_call 3 15 "$dumpfile", "vvp/counter4.vcd" {0 0 0};
    %vpi_call 3 16 "$dumpvars", 32'sb00000000000000000000000000000001, S_00823190 {0 0 0};
    %end;
    .thread T_5;
