#! c:/iverilog-x86/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_02782390 .scope module, "DFlipFlop" "DFlipFlop" 2 1;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "q"
    .port_info 1 /OUTPUT 1 "qBar"
    .port_info 2 /INPUT 1 "D"
    .port_info 3 /INPUT 1 "clk"
    .port_info 4 /INPUT 1 "rst"
L_0102d708 .functor NOT 1, v01029820_0, C4<0>, C4<0>, C4<0>;
o02783fec .functor BUFZ 1, C4<z>; HiZ drive
v0102a5e8_0 .net "D", 0 0, o02783fec;  0 drivers
o02784004 .functor BUFZ 1, C4<z>; HiZ drive
v0102ec70_0 .net "clk", 0 0, o02784004;  0 drivers
v01029820_0 .var "q", 0 0;
v0102be98_0 .net "qBar", 0 0, L_0102d708;  1 drivers
o0278404c .functor BUFZ 1, C4<z>; HiZ drive
v0102bef0_0 .net "rst", 0 0, o0278404c;  0 drivers
E_0102c208/0 .event negedge, v0102bef0_0;
E_0102c208/1 .event posedge, v0102ec70_0;
E_0102c208 .event/or E_0102c208/0, E_0102c208/1;
S_02782460 .scope module, "testBench" "testBench" 3 5;
 .timescale 0 0;
v027805b0_0 .net "clk", 0 0, v0102bf48_0;  1 drivers
v02780558_0 .net "q", 3 0, v010238d0_0;  1 drivers
v02780768_0 .net "rst", 0 0, v02783298_0;  1 drivers
S_02783170 .scope module, "aTester" "Tester" 3 10, 3 20 0, S_02782460;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "q"
    .port_info 1 /OUTPUT 1 "clk"
    .port_info 2 /OUTPUT 1 "rst"
P_0102c690 .param/l "stimDelay" 0 3 24, +C4<00000000000000000000000000010100>;
v0102bf48_0 .var "clk", 0 0;
v0102bfa0_0 .var/i "i", 31 0;
v02783240_0 .net "q", 3 0, v010238d0_0;  alias, 1 drivers
v02783298_0 .var "rst", 0 0;
S_01023800 .scope module, "counter" "Counter4" 3 9, 4 1 0, S_02782460;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "q"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "rst"
v027832f0_0 .net "clk", 0 0, v0102bf48_0;  alias, 1 drivers
v010238d0_0 .var "q", 3 0;
v027800e0_0 .net "rst", 0 0, v02783298_0;  alias, 1 drivers
E_0102c730 .event posedge, v02783298_0, v0102bf48_0;
    .scope S_02782390;
T_0 ;
    %wait E_0102c208;
    %load/vec4 v0102bef0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01029820_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0102a5e8_0;
    %store/vec4 v01029820_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01023800;
T_1 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v010238d0_0, 0, 4;
    %end;
    .thread T_1;
    .scope S_01023800;
T_2 ;
    %wait E_0102c730;
    %load/vec4 v027800e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v010238d0_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v010238d0_0;
    %cmpi/ne 15, 0, 4;
    %jmp/0xz  T_2.2, 4;
    %load/vec4 v010238d0_0;
    %addi 1, 0, 4;
    %assign/vec4 v010238d0_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v010238d0_0, 0;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_02783170;
T_3 ;
    %vpi_call 3 29 "$display", "\011\011 clk rst \011 q \011 Time " {0 0 0};
    %vpi_call 3 30 "$monitor", "\011\011 %b\011 %b \011 %b", v0102bf48_0, v02783298_0, v02783240_0, $time {0 0 0};
    %end;
    .thread T_3;
    .scope S_02783170;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0102bf48_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v02783298_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0102bf48_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v02783298_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0102bf48_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v02783298_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0102bfa0_0, 0, 32;
T_4.0 ;
    %load/vec4 v0102bfa0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.1, 5;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0102bf48_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0102bf48_0, 0, 1;
    %load/vec4 v0102bfa0_0;
    %addi 1, 0, 32;
    %store/vec4 v0102bfa0_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %delay 40, 0;
    %vpi_call 3 44 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_02782460;
T_5 ;
    %vpi_call 3 15 "$dumpfile", "vvp/counter4.vcd" {0 0 0};
    %vpi_call 3 16 "$dumpvars", 32'sb00000000000000000000000000000001, S_01023800 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 5;
