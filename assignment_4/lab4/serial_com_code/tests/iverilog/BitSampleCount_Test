v00705208_0 .var "bic", 0 0;
v00705260_0 .net "clk", 0 0, v0070b418_0;  alias, 1 drivers
v007052b8_0 .net "counterOut", 3 0, v007036e8_0;  1 drivers
v00705310_0 .net "rstBSC", 0 0, o01fe4024;  alias, 0 drivers
v01fe0fb8_0 .var "srcc", 0 0;
E_0070cb68 .event posedge, v0070b418_0;
S_007035c0 .scope module, "counter" "Counter4" 3 7, 4 1 0, S_01fe3b10;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "q"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "rst"
v00703690_0 .net "clk", 0 0, v0070b418_0;  alias, 1 drivers
v007036e8_0 .var "q", 3 0;
v00703740_0 .net "rst", 0 0, o01fe4024;  alias, 0 drivers
E_0070cc08 .event posedge, v01fe3a60_0, v0070b418_0;
    .scope S_007035c0;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v007036e8_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_007035c0;
T_1 ;
    %wait E_0070cc08;
    %load/vec4 v00703740_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v007036e8_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v007036e8_0;
    %cmpi/ne 15, 0, 4;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v007036e8_0;
    %addi 1, 0, 4;
    %assign/vec4 v007036e8_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v007036e8_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_01fe3b10;
T_2 ;
    %wait E_0070cb68;
    %load/vec4 v007052b8_0;
    %pad/u 32;
    %cmpi/e 15, 0, 32;
    %jmp/0xz  T_2.0, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v00705208_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v01fe0fb8_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v007052b8_0;
    %pad/u 32;
    %cmpi/e 7, 0, 32;
    %jmp/0xz  T_2.2, 4;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v00705208_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v01fe0fb8_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v00705208_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v01fe0fb8_0, 0;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_01fe2880;
T_3 ;
    %vpi_call 2 28 "$display", "\011\011 clk rst \011 srcc \011 bic \011 Time " {0 0 0};
    %vpi_call 2 29 "$monitor", "\011\011 %b\011 %b \011 %b \011 %b", v0070b418_0, v01fe3a08_0, v01fe3ab8_0, v0070b5b0_0, $time {0 0 0};
    %end;
    .thread T_3;
    .scope S_01fe2880;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0070b418_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01fe3a08_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0070b418_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v01fe3a08_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0070b418_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v01fe3a08_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v00708be0_0, 0, 32;
T_4.0 ;
    %load/vec4 v00708be0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.1, 5;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0070b418_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0070b418_0, 0, 1;
    %load/vec4 v00708be0_0;
    %addi 1, 0, 32;
    %store/vec4 v00708be0_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %delay 40, 0;
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_01fe27b0;
T_5 ;
    %vpi_call 2 14 "$dumpfile", "vvp/bitsamplecount.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000001, S_01fe3b10 {0 0 0};
