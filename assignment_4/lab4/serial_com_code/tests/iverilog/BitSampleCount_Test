v011c5590_0 .net "rstBSC", 0 0, o02794024;  alias, 0 drivers
v027bb030_0 .var "srcc", 0 0;
E_011cd4e8 .event posedge, v011cb708_0;
S_011c3800 .scope module, "counter" "Counter4" 3 7, 4 1 0, S_02793b50;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "q"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "rst"
v011c38d0_0 .net "clk", 0 0, v011cb708_0;  alias, 1 drivers
v011c3928_0 .var "q", 3 0;
v011c3980_0 .net "rst", 0 0, o02794024;  alias, 0 drivers
E_011cd3f8 .event posedge, v02793aa0_0, v011cb708_0;
    .scope S_011c3800;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v011c3928_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_011c3800;
T_1 ;
    %wait E_011cd3f8;
    %load/vec4 v011c3980_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v011c3928_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v011c3928_0;
    %cmpi/ne 15, 0, 4;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v011c3928_0;
    %addi 1, 0, 4;
    %assign/vec4 v011c3928_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v011c3928_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_02793b50;
T_2 ;
    %wait E_011cd4e8;
    %load/vec4 v011c5538_0;
    %pad/u 32;
    %cmpi/e 15, 0, 32;
    %jmp/0xz  T_2.0, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v011c5488_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v027bb030_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v011c5538_0;
    %pad/u 32;
    %cmpi/e 7, 0, 32;
    %jmp/0xz  T_2.2, 4;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v011c5488_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v027bb030_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v011c5488_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v027bb030_0, 0;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
