S_01fe27b0 .scope module, "testBench" "testBench" 2 4;
 .timescale 0 0;
v01fe1010_0 .net "bic", 0 0, v00705208_0;  1 drivers
v01fe1170_0 .net "clk", 0 0, v0070b418_0;  1 drivers
o01fe4024 .functor BUFZ 1, C4<z>; HiZ drive
v01fe0e00_0 .net "rstBSC", 0 0, o01fe4024;  0 drivers
v01fe0da8_0 .net "srcc", 0 0, v01fe0fb8_0;  1 drivers
S_01fe2880 .scope module, "aTester" "Tester" 2 9, 2 19 0, S_01fe27b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "srcc"
    .port_info 1 /INPUT 1 "bic"
    .port_info 2 /OUTPUT 1 "rstBSC"
    .port_info 3 /OUTPUT 1 "clk"
P_0070caf0 .param/l "stimDelay" 0 2 23, +C4<00000000000000000000000000010100>;
v0070b5b0_0 .net "bic", 0 0, v00705208_0;  alias, 1 drivers
v0070b418_0 .var "clk", 0 0;
v00708be0_0 .var/i "i", 31 0;
