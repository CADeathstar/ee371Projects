    .scope S_027928c0;
T_3 ;
    %vpi_call 2 28 "$display", "\011\011 clk rst \011 srcc \011 bic \011 Time " {0 0 0};
    %vpi_call 2 29 "$monitor", "\011\011 %b\011 %b \011 %b \011 %b", v011cb708_0, v02793a48_0, v02793af8_0, v011cbf70_0, $time {0 0 0};
    %end;
    .thread T_3;
    .scope S_027928c0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v011cb708_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v02793a48_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v011cb708_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v02793a48_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v011cb708_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v02793a48_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v011c9540_0, 0, 32;
T_4.0 ;
    %load/vec4 v011c9540_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.1, 5;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v011cb708_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v011cb708_0, 0, 1;
    %load/vec4 v011c9540_0;
    %addi 1, 0, 32;
    %store/vec4 v011c9540_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %delay 40, 0;
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
