#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_003DD778 .scope module, "testBench" "testBench" 2 4;
 .timescale 0 0;
v01E23800_0 .net "clk", 0 0, v003DEF68_0; 1 drivers
v01E23858_0 .net "done", 0 0, v003DEDC8_0; 1 drivers
v01E238B0_0 .net "rst", 0 0, v003DE3B8_0; 1 drivers
v003DB9C0_0 .net "stopVal", 0 2, v003DED18_0; 1 drivers
S_003DD558 .scope module, "t" "Timer" 2 9, 3 1, S_003DD778;
 .timescale 0 0;
v003DED70_0 .alias "clk", 0 0, v01E23800_0;
v003DEDC8_0 .var "done", 0 0;
v01E232B8_0 .var "q", 0 2;
v01E23310_0 .alias "rst", 0 0, v01E238B0_0;
v01E23368_0 .alias "stopVal", 0 2, v003DB9C0_0;
E_003DC798 .event posedge, v003DEF68_0;
E_003DC538 .event edge, v01E232B8_0, v003DED18_0;
S_003DDBB8 .scope module, "aTester" "Tester" 2 10, 2 21, S_003DD778;
 .timescale 0 0;
P_003DC51C .param/l "stimDelay" 2 29, +C4<010100>;
v003DEF68_0 .var "clk", 0 0;
v003DE308_0 .alias "done", 0 0, v01E23858_0;
v003DE360_0 .var/i "i", 31 0;
v003DE3B8_0 .var "rst", 0 0;
v003DED18_0 .var "stopVal", 0 2;
    .scope S_003DD558;
T_0 ;
    %wait E_003DC538;
    %load/v 8, v01E232B8_0, 3;
    %load/v 11, v01E23368_0, 3;
    %cmp/u 8, 11, 3;
    %mov 8, 4, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v003DEDC8_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_003DD558;
T_1 ;
    %wait E_003DC798;
    %load/v 8, v01E23310_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 3, 0;
    %assign/v0 v01E232B8_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v01E232B8_0, 3;
    %load/v 11, v01E23368_0, 3;
    %cmp/u 8, 11, 3;
    %inv 4, 1;
    %jmp/0xz  T_1.2, 4;
    %load/v 8, v01E232B8_0, 3;
    %mov 11, 0, 29;
    %addi 8, 1, 32;
    %ix/load 0, 3, 0;
    %assign/v0 v01E232B8_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %load/v 8, v01E232B8_0, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v01E232B8_0, 0, 8;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_003DDBB8;
T_2 ;
    %vpi_call 2 35 "$display", "\011\011 clk rst \011 done \011 Time ";
    %vpi_call 2 36 "$monitor", "\011\011 %b\011 %b \011 %b", v003DEF68_0, v003DE3B8_0, v003DE308_0, $time;
    %end;
    .thread T_2;
    .scope S_003DDBB8;
T_3 ;
    %set/v v003DEF68_0, 0, 1;
    %set/v v003DE3B8_0, 0, 1;
    %movi 8, 4, 3;
    %set/v v003DED18_0, 8, 3;
    %delay 20, 0;
    %set/v v003DEF68_0, 1, 1;
    %set/v v003DE3B8_0, 1, 1;
    %delay 20, 0;
    %set/v v003DEF68_0, 0, 1;
    %set/v v003DE3B8_0, 0, 1;
    %delay 20, 0;
    %set/v v003DEF68_0, 1, 1;
    %set/v v003DE360_0, 0, 32;
T_3.0 ;
    %load/v 8, v003DE360_0, 32;
   %cmpi/s 8, 16, 32;
    %jmp/0xz T_3.1, 5;
    %delay 20, 0;
    %set/v v003DEF68_0, 0, 1;
    %delay 20, 0;
    %set/v v003DEF68_0, 1, 1;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v003DE360_0, 32;
    %set/v v003DE360_0, 8, 32;
    %jmp T_3.0;
T_3.1 ;
    %delay 40, 0;
    %vpi_call 2 52 "$finish";
    %end;
    .thread T_3;
    .scope S_003DD778;
T_4 ;
    %vpi_call 2 16 "$dumpfile", "timer.vcd";
    %vpi_call 2 17 "$dumpvars", 2'sb01, S_003DD558;
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
