#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_003DDD50 .scope module, "testBench" "testBench" 2 4;
 .timescale 0 0;
v003DBBB0_0 .net "clk", 0 0, v003D88A0_0; 1 drivers
v003DBC08_0 .net "outerDoor", 0 0, v01E83718_0; 1 drivers
v003DBC60_0 .net "outerDoorCtrl", 0 0, v01E83058_0; 1 drivers
v003DBCB8_0 .net "pressurized", 0 0, v01E830B0_0; 1 drivers
v003DBD10_0 .net "rst", 0 0, v01E83108_0; 1 drivers
S_003DDCC8 .scope module, "oDoor" "OuterDoor" 2 9, 3 1, S_003DDD50;
 .timescale 0 0;
v01E83668_0 .alias "clk", 0 0, v003DBBB0_0;
v01E836C0_0 .var "ns", 0 0;
v01E83718_0 .var "outerDoor", 0 0;
v01E83770_0 .alias "outerDoorCtrl", 0 0, v003DBC60_0;
v003DBB00_0 .alias "pressurized", 0 0, v003DBCB8_0;
v003DBB58_0 .alias "rst", 0 0, v003DBD10_0;
E_003D92C8/0 .event negedge, v01E83108_0;
E_003D92C8/1 .event posedge, v003D88A0_0;
E_003D92C8 .event/or E_003D92C8/0, E_003D92C8/1;
E_003D92E8 .event edge, v01E83058_0, v01E830B0_0;
S_003DDDD8 .scope module, "aTester" "Tester" 2 10, 2 20, S_003DDD50;
 .timescale 0 0;
P_003D936C .param/l "stimDelay" 2 27, +C4<010100>;
v003D88A0_0 .var "clk", 0 0;
v003D88F8_0 .alias "outerDoor", 0 0, v003DBC08_0;
v01E83058_0 .var "outerDoorCtrl", 0 0;
v01E830B0_0 .var "pressurized", 0 0;
v01E83108_0 .var "rst", 0 0;
    .scope S_003DDCC8;
T_0 ;
    %wait E_003D92E8;
    %load/v 8, v01E83770_0, 1;
    %load/v 9, v003DBB00_0, 1;
    %and 8, 9, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v01E836C0_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_003DDCC8;
T_1 ;
    %wait E_003D92C8;
    %load/v 8, v003DBB58_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v01E83718_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v01E836C0_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v01E83718_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_003DDDD8;
T_2 ;
    %vpi_call 2 31 "$display", "\011\011 clk rst \011 outerDoor \011 Time ";
    %vpi_call 2 32 "$monitor", "\011\011 %b\011 %b \011 %b", v003D88A0_0, v01E83108_0, v003D88F8_0, $time;
    %end;
    .thread T_2;
    .scope S_003DDDD8;
T_3 ;
    %set/v v003D88A0_0, 0, 1;
    %set/v v01E83108_0, 0, 1;
    %set/v v01E83058_0, 0, 1;
    %set/v v01E830B0_0, 0, 1;
    %set/v v003D88A0_0, 0, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 1, 1;
    %set/v v01E83108_0, 1, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 0, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 1, 1;
    %set/v v01E83058_0, 1, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 0, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 1, 1;
    %set/v v01E830B0_0, 1, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 0, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 1, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 0, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 1, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 0, 1;
    %delay 20, 0;
    %set/v v003D88A0_0, 1, 1;
    %delay 20, 0;
    %delay 40, 0;
    %vpi_call 2 51 "$finish";
    %end;
    .thread T_3;
    .scope S_003DDD50;
T_4 ;
    %vpi_call 2 15 "$dumpfile", "outerDoor.vcd";
    %vpi_call 2 16 "$dumpvars", 2'sb01, S_003DDCC8;
    %end;
    .thread T_4;
